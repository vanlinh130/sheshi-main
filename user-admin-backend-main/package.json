{
  "name": "api-sheshi",
  "version": "1.0.0",
  "description": "api-sheshi",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist && mkdir dist",
    "clean-log": "rimraf logs && mkdir logs",
    "build": "npm run clean && babel app --out-dir dist && ncp app/template dist/template",
    "start": "env-cmd nodemon --exec babel-node app/server.js",
    "serve": "node dist/server.js",
    "test": "mocha --recursive --timeout 100000 --exit",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "seed-file": "sequelize-cli db:seed --seed=",
    "undo-seed-file": "sequelize-cli db:seed:undo --seed",
    "migrate": "sequelize-cli db:migrate"
  },
  "keywords": [
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Nguyen Tan Duy",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.778.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.0",
    "bignumber.js": "^9.0.1",
    "body-parser": "^1.20.0",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "crypto": "^1.0.1",
    "dateformat": "^5.0.3",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.8",
    "ethereumjs-tx": "1.3.7",
    "express": "^4.18.1",
    "fs": "0.0.2",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "md5": "^2.2.1",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mustache": "^4.0.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.7.7",
    "numeral": "^2.0.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "qs": "^6.11.0",
    "radium": "^0.26.2",
    "randomstring": "^1.2.2",
    "request-promise": "^4.2.6",
    "sequelize": "^6.2.4",
    "sharp": "^0.30.7",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-watch": "^4.0.2",
    "mocha": "^8.0.1",
    "ncp": "^2.0.0",
    "nodemon": "^1.19.4",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0"
  },
  "nyc": {
    "_comment": "Purpose of this part: to do code coverage within Mocha/Chai using Istanbul",
    "register": "babel-register",
    "include": [
      "app/**/*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "**/test/**",
      "**/coverage/**"
    ],
    "all": true
  }
}
